       <string notr="true">/* style.css */

/* Table View */
QTableView {
    border: 1px solid #2c3e50; /* Border around the table view */
    selection-background-color: #3498db; /* Background color of selected rows */
    color: #000000; /* Text color of selected rows */

}

/* Table Header */
QHeaderView::section {
    background-color: #2c3e50; /* Header background color */
    color: white; /* Header text color */
    padding: 4px; /* Padding around header text */
    border: 1px solid #34495e; /* Border around header */
    border-style: outset; /* Border style */
    border-radius: 3px; /* Border radius */
}

/* Table Rows */
QTableView QScrollBar:vertical {
    border: 1px solid #ccc; /* Vertical scrollbar border */
    background: #f0f0f0; /* Vertical scrollbar background color */
}

QTableView QScrollBar::handle:vertical {
    background: #95a5a6; /* Vertical scrollbar handle background color */
}

QTableView QScrollBar::add-line:vertical,
QTableView QScrollBar::sub-line:vertical {
    background: #ecf0f1; /* Vertical scrollbar line background color */
    subcontrol-position: top; /* Position of the lines */
    subcontrol-origin: margin; /* Origin of the lines */
}
